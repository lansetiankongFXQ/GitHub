# -*- coding: utf-8 -*-

# Define here the models for your scraped items
#
# See documentation in:
# https://doc.scrapy.org/en/latest/topics/items.html

import scrapy
from scrapy import Field


class WanfangCategoryItem(scrapy.Item):
    _id = Field()
    category_id = Field()
    category_name = Field()
    category_type = Field()


class WanfangExaminationItem(scrapy.Item):
    _id = Field()
    name = Field()
    alias_name = Field()
    en_name = Field()
    ab_name = Field()
    description = Field()
    category_id = Field()
    category_name = Field()
    category_type = Field()

